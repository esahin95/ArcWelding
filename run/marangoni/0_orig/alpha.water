FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top
    {
        type            inletOutlet;
        value           uniform 0;
        inletValue      uniform 0;
    }

    right
    {
        type           zeroGradient;
    }
    
    left
    {
        type           zeroGradient;
    }

    bottom
    {
        type           zeroGradient;
    }

    frontAndBack
    {
        type           empty;
    }
}


