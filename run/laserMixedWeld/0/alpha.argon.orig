FoamFile
{
    format      ascii;
    class       volScalarField;
    object      alpha.argon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    zmax
    {
        type            inletOutlet;
        value           uniform 1;
        inletValue      uniform 1;
    }

    "(zmin|xmin|xmax|ymin|ymax)"
    {
        type            zeroGradient;
    }
}


