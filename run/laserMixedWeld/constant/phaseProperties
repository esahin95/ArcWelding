FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases          (argon steel copper);

// surface tension between argon and steel
sigma12
{
    type constant;
    sigma 0.05;
}

// surface tension between argon and copper
sigma13
{
    type constant;
    sigma 0.04;
}

// Diffusivity between miscible phases
D23 3e-05;

