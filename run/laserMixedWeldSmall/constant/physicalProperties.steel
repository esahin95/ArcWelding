FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

viscosityModel  constant;

nu              1.1e-6;
rho             7249.2;

Cp              726;
kappa           28.951;
beta            0.0;

L               0;
alphaSolid 
{
    type        errorFunction;
    Tliq        1700;
    Tsol        1400;
}


