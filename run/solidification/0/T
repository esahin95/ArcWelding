FoamFile
{
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 295;

boundaryField
{
    top
    {
        type            zeroGradient;
    }

    right
    {
        type           fixedValue;
        value          uniform 295;
    }
    
    left
    {
        type           fixedValue;
        value          uniform 300;
    }

    bottom
    {
        type           zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}


