FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

viscosityModel  Newtonian;

nu              3.2e-06;

rho             1562.5;

Cp              500;

kappa           12.5;

beta            0;

Tliq            300;
Tsol            298;
L               1000;
DC              10000;


