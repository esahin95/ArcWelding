FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

viscosityModel  constant;

nu              1e-6;
rho             998;

Cp		        4182;
kappa	        0.598;
beta	        1e-4;

L               150;
alphaSolid
{
    type errorFunction;
    Tliq 292.0;
    Tsol 288.0;
}

