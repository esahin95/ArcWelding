FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    /*
    zmax
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }
    */

    zmax
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

    "(zmin|xmin|xmax|ymin|ymax)"
    {
        type            noSlip;
    }
}


